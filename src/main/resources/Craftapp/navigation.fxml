<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefWidth="335.2" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="30.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <TextField id="search" fx:id="searchBar" alignment="CENTER" focusTraversable="false" prefHeight="25.0" prefWidth="265.0" promptText="Search" />
                  <Button fx:id="searchButton" alignment="CENTER" contentDisplay="CENTER" focusTraversable="false" mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Search" />
               </children>
            </HBox>
            <HBox layoutY="35.0" prefHeight="30.0" prefWidth="335.0" spacing="5.0">
               <children>
                  <Button fx:id="homeButton" focusTraversable="false" mnemonicParsing="false" prefWidth="50.0" text="Home" />
                  <Button fx:id="addButton" focusTraversable="false" mnemonicParsing="false" prefWidth="50.0" text="Add" />
                  <ChoiceBox fx:id="choiceBox" focusTraversable="false" prefHeight="25.0" prefWidth="160.0" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
